{
  "name": "jrlo",
  "version": "0.17.0",
  "description": "JIRA and Trello CLI",
  "bin": {
    "jrlo": "./dist/index.js"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsc --sourceMap false",
    "build:watch": "tsc --watch",
    "start": "tsc && node ./dist/index.js",
    "lint": "tslint -c tslint.json 'src/**/*.ts'",
    "test": "jest",
    "test:cov": "jest --coverage",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "release": "standard-version"
  },
  "author": {
    "name": "sgyio",
    "email": "sgy@sgy.io",
    "url": "https://github.com/sgyio"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sgyio/jrlo"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/chalk": "^2.2.0",
    "@types/jest": "^22.2.3",
    "@types/node": "^10.5.7",
    "@types/opn": "^5.1.0",
    "@types/update-notifier": "^2.2.0",
    "@types/yargs": "^11.1.1",
    "ajv": "^6.9.1",
    "ajv-keywords": "^3.2.0",
    "babel-core": "^7.0.0-bridge.0",
    "dumper.js": "^0.1.1",
    "husky": "^1.0.0-rc.13",
    "jest": "^24.1.0",
    "standard-version": "^6.0.1",
    "ts-jest": "^23.1.4",
    "tslint": "^5.11.0",
    "typescript": "^3.0.1",
    "validate-commit-msg": "^2.14.0"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "boxen": "^1.3.0",
    "chalk": "^2.4.1",
    "configstore": "^4.0.0",
    "inquirer": "^6.1.0",
    "inversify": "^4.10.0",
    "keytar": "^4.2.1",
    "listr": "^0.14.1",
    "nconf": "^0.10.0",
    "opn": "^5.3.0",
    "promise-streams": "^2.1.1",
    "reflect-metadata": "^0.1.12",
    "rxjs": "^6.2.2",
    "split2": "^3.0.0",
    "table": "^4.0.3",
    "update-notifier": "^2.5.0",
    "yargs": "^12.0.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "test",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}

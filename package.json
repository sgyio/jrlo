{
  "name": "jrlo",
  "version": "0.17.1",
  "description": "JIRA and Trello CLI",
  "bin": {
    "jrlo": "./dist/index.js"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsc --sourceMap false",
    "build:watch": "tsc --watch",
    "start": "tsc && node ./dist/index.js",
    "lint": "tslint -c tslint.json 'src/**/*.ts'",
    "test": "jest",
    "test:cov": "jest --coverage",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "release": "standard-version"
  },
  "author": {
    "name": "sgyio",
    "email": "sgy@sgy.io",
    "url": "https://github.com/sgyio"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sgyio/jrlo"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/chalk": "^2.2.0",
    "@types/jest": "^22.2.3",
    "@types/node": "^10.14.19",
    "@types/opn": "^5.5.0",
    "@types/update-notifier": "^2.5.0",
    "@types/yargs": "^11.1.2",
    "dumper.js": "^0.1.1",
    "husky": "^1.3.1",
    "jest": "^24.9.0",
    "ts-jest": "^23.10.5",
    "tslint": "^5.20.0",
    "typescript": "^3.6.3"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "boxen": "^1.3.0",
    "chalk": "^2.4.2",
    "configstore": "^4.0.0",
    "inquirer": "^6.5.2",
    "inversify": "^4.13.0",
    "keytar": "^4.13.0",
    "listr": "^0.14.3",
    "nconf": "^0.10.0",
    "opn": "^5.5.0",
    "promise-streams": "^2.1.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.5.3",
    "split2": "^3.1.1",
    "table": "^4.0.3",
    "update-notifier": "^2.5.0",
    "yargs": "^12.0.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "test",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}